      "tooltip": null,
      "showButton": false,
      "terminal": {
        "cwd": "/config/workspace/transform",
        "singleInstance": true,
        "clear": true,
        "focus": false,
        "name": null
      }
    }
  ],
  "betterStatusBar.dropdowns": [
    {
      "id": "Dropdown",
      "label": "Dropdown",
      // The id or the label can be used to show the commands.
      "commands": ["5", "üó≥Ô∏è Prod metadata"],
